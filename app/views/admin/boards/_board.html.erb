<tr id="board-<%= board.id %>">
  <td><%= board.id %></td>
  <td>
    <%= link_to board.title, admin_board_path(id: board.id) %>
  </td>
  <td><%= board.user.decorate.full_name %></td>
  <td><%= l board.created_at, format: :long %></td>
  <td>
    <%=
      link_to(
        content_tag(:i, '', class: 'fas fa-edit'),
        edit_admin_board_path(id: board.id),
        class: 'btn btn-link'
      )
    %>
    <button type="button" class="btn btn-link" data-toggle="modal" data-target="#js-board-delete-confirm-modal" data-delete-url="<%= admin_board_path(id: board.id) %>">
      <i class="fas fa-trash"></i>
    </button>
  </td>
</tr>
  