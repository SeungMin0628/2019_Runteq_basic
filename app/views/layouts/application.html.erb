<!DOCTYPE html>
<html>
  <head>
    <title>RunteqNormal</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if logged_in? %>
      <!-- for user -->
      <%= render partial: "shared/header" %>
    <% else %>
      <!-- for guest -->
      <%= render partial: "shared/before_login_header" %>
    <% end %>
    
    <%= yield %>
    <%= render partial: "shared/footer" %>
  </body>
</html>
